{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "src/",
    // "include": ["src"],
    "paths": {
      // "~/*": "./*"
      "~/components/*": ["components/*"]
    }
  }
}
